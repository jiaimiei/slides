{
	"$schema": "../node_modules/@tauri-apps/cli/schema.json",
	"build": {
		"beforeBuildCommand": "yarn build",
		"beforeDevCommand": "yarn dev",
		"devPath": "http://localhost:5173",
		"distDir": "../build"
	},
	"package": {
		"productName": "Slides",
		"version": "1.0.1"
	},
	"tauri": {
		"allowlist": {
			"all": true,
			"fs": {
				"scope": {
					"allow": [
						"**/*"
					],
					"requireLiteralLeadingDot": false
				}
			},
			"protocol": {
				"asset": true,
				"assetScope": {
					"allow": [
						"**/*"
					],
					"requireLiteralLeadingDot": false
				}
			}
		},
		"bundle": {
			"active": true,
			"category": "Education",
			"copyright": "",
			"deb": {
				"depends": []
			},
			"externalBin": [],
			"icon": [
				"icons/32x32.png",
				"icons/128x128.png",
				"icons/128x128@2x.png",
				"icons/icon.icns",
				"icons/icon.ico"
			],
			"identifier": "app.j.slides",
			"longDescription": "",
			"macOS": {
				"entitlements": null,
				"exceptionDomain": "",
				"frameworks": [],
				"providerShortName": null,
				"signingIdentity": null,
				"minimumSystemVersion": "11.0"
			},
			"resources": [],
			"shortDescription": "",
			"targets": "all",
			"windows": {
				"certificateThumbprint": null,
				"digestAlgorithm": "sha256",
				"timestampUrl": ""
			}
		},
		"security": {
			"csp": null
		},
		"updater": {
			"active": true,
			"dialog": false,
			"endpoints": ["https://github.com/jiaimiei/slides/releases/latest/download/latest.json"],
			"pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDU4OEJBN0RBN0FCRkQ4MEQKUldRTjJMOTYycWVMV08vTVd3MXVpb242aUxaeGNqa3NLWVd3RlMvaTJlVCtxNWl1U1FRNFY4OHEK"
		},
		"windows": [
			{
				"fullscreen": false,
				"height": 600,
				"resizable": true,
				"title": "Slides",
				"width": 800
			}
		]
	}
}